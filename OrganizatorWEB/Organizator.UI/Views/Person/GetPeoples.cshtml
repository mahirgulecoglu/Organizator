@model List<Organizator.Entity.EventPeople>

@{
    ViewBag.Title = "PersonDetail";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<br /><br /><br /><br />

<section class="ftco-section contact-section">
    <div class="container">
        <div class="row d-flex mb-5 contact-info" style="font-size:x-large">
            <div class="col-md-12 mb-4">
                <h2 class="h1">Participants</h2>
            </div>
            <div class="w-100"></div>
            @foreach (var item in Model)
            {
                <div class="col-md-12">
                    <p><span>@item.Person.FirstName @item.Person.LastName </span>için Ayrılan Koltuk Sayısı : @item.TotalPerson</p>
                </div>

            }
        </div>
    </div>
</section>
